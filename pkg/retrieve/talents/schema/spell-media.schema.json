{
  "type": "object",
  "required": [
    "id",
    "assets"
  ],
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1
    },
    "assets": {
      "type": "array",
      "minItems": 1,
      "maxItems": 1,
      "items": {
        "type": "object",
        "required": [
          "key",
          "value"
        ],
        "properties": {
          "key": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      },
      "contains": {
        "type": "object",
        "required": [
          "key",
          "value"
        ],
        "properties": {
          "key": {
            "type": "string",
            "pattern": "^icon$"
          },
          "value": {
            "type": "string"
          }
        }
      }
    }
  }
}
